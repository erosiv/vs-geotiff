{
	"name": "vs-geotiff",
	"displayName": "vs-geotiff",
	"description": "GeoTIFF Inspector",
	"version": "0.0.1",
	"publisher": "erosiv",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/erosiv/vs-geotiff"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "vsGeoTIFF.GeoTIFF",
				"displayName": "GeoTIFF Inspector",
				"selector": [
					{
						"filenamePattern": "*.tiff"
					},
					{
						"filenamePattern": "*.tif"
					}
				]
			}
		],
		"commands": [
			{
				"command": "vsGeotiff.GeoTIFF.shade",
				"title": "Shade .TIFF File"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/node": "^20",
		"@types/vscode": "^1.73.0",
		"eslint": "^9.13.0",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.16.0"
	},
	"dependencies": {
		"erosiv-tiff": "^6.1.4"
	}
}